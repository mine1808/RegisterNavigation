<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Registerbutton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">50dp</item>
        <item name="android:layout_marginHorizontal">30dp</item>
        <item name="android:text">Register</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@drawable/rounded_btn_register</item>
        <item name="backgroundTint">#525BFF</item>
    </style>
    <style name="Logoutbutton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">50dp</item>
        <item name="android:layout_marginHorizontal">30dp</item>
        <item name="android:text">Logout</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@drawable/rounded_btn_register</item>
        <item name="backgroundTint">#525BFF</item>
    </style>
</resources>